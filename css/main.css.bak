
/*contUp*/

.contUp{
	min-width:1024px;
	height:460px;
	margin:0 auto;}

	/*animation css*/

	.contUp .animation{
		width:100%;
		height:460px;
		position:absolute;
		left:0;
		overflow:hidden;}

		.contUp .animation ul{
			width:300%;
			height:460px;}

			.contUp .animation ul li{
				width:100%;
				height:462px;
				position:absolute;
				transition:all 2s;
				left:1920px;}

				.contUp .animation ul li:nth-of-type(1){
					background:url('/img/main1.png') 50% 50%;
					left:0; }

				.contUp .animation ul li:nth-of-type(2){
					background:url('/img/main2.png') 50% 50%; }

				.contUp .animation ul li:nth-of-type(3){
					background:url('/img/main3.png') 50% 50%; }

	/*upCont css*/
	.contUp .upCont{
		width:1024px;
		height:462px;
		position:relative;
		margin:0 auto;
		display:flex;}

		.contUp .upCont > span{
			font-size:5em;
			font-family:'돋움';
			color:#444;
			cursor:pointer;
			width:50px;
			height:50px;
			line-height:60%;}

		.contUp .upCont > span:hover{
			color:#fff;}

			.contUp .upCont > span:nth-of-type(1){
				margin:207px 450px 0 12px;}

			.contUp .upCont > span:nth-of-type(2){
				margin:207px 12px 0 450px;}

		.contUp .upCont .login{
			background:#fff;
			display:inline-block;
			width:230px;
			border-radius:10px;
			overflow:hidden;
			box-shadow:2px 2px 2px #444;
			position:absolute;
			z-index:100;
			top:100px;
			right:100px;}

			.contUp .upCont .login strong{
				display:block;
				background:url('/img/log_title.png');
				text-align:left;
				padding-left:20px;
				font-size:1.1em;
				font-weight:bold;
				font-family:'malgun gothic';
				padding-top:10px;
				height:40px;
				color:#fff;	}

				.contUp .upCont .login > div{
					padding:15px 20px;}

					.contUp .upCont .login div dl{
						padding:5px 0;}

						.contUp .upCont .login div dl dt,
						.contUp .upCont .login div dl dd{
							display:inline-block;
							vertical-align:top;
							text-align:center;
							padding-top:5px;}

						.contUp .upCont .login div dl dt label{
							width:50px;
							font-size:0.75em;
							font-family:'돋움';
							display:inline-block;
							padding:7px 10px 0 20px;
							color:#666;}

						.contUp .upCont .login div dl dd input{
							width:118px;
							height:25px;
							border:1px solid #ccc;
							}

						.contUp .upCont .login .btnBox{
							text-align:center;
							padding:10px 0;}

							.contUp .upCont .login .btnBox input[type=submit],
							.contUp .upCont .login .btnBox input[type=button]{
								width:188px;
								height:42px;
								border-radius:5px;
								color:#fff;
								font-weight:bold;}
							
							.contUp .upCont .login div.btnBox input:nth-of-type(1){
								background:url('/img/login.png');
								border:1px solid #017b82;
								margin:5px 0;}

							.contUp .upCont .login div input:nth-of-type(2){
								background:url('/img/join.png');
								border:1px solid #535353;}
	/*slideBtn css*/
	.slideBtn{
		width:100%;
		text-align:center;
		position:absolute;
		left:0;
		bottom:0;}

		.slideBtn span{
			display:inline-block;
			width:10px;
			height:10px;
			border-radius:15px ;
			background:#fff;
			overflow:hidden;
			line-height:300%;
			box-shadow:inset 3px 2px 3px #777; }

	/*cont center*/

	.contCenter{
		min-width:1024px;
		background:#2c2c2c;
		height:60px;
		padding:10px 0; }

			.contCenter .wrapCenter{
				width:1024px;
				margin:0 auto;
				display:flex; }

				.contCenter .wrapCenter div,
				.contCenter .wrapCenter div > figure{
					display:flex; }

				.contCenter .wrapCenter div > figure img{
					transition:all .4s ease-in-out; }
				

				.contCenter .wrapCenter div > figure img:hover{
					transform:rotate(360deg);}

					.contCenter .wrapCenter div > figure figcaption{
						padding-top:20px;
						color:#fff;
						width:100px;
						padding-left:8px;}

						.contCenter .wrapCenter div > figure figcaption a{
							color:#fff; }

	
			.contCenter .wrapCenter form{
				padding-top:15px;
				padding-left:200px;}

				.contCenter .wrapCenter form input[type=text]{
					width:250px;
					height:20px;
					padding:5px;
					border:none;
					float:left;}

				.contCenter .wrapCenter form input[type=submit]{
					background:#13858c;
					padding:6px 15px;
					border:none;
					color:#fff;
					font-weight:bold;}

	/*content*/

	.content{
		min-width:1024px;
		background:url('/img/main_down.png') 50% 50%;
		padding-top:30px;
		height:220px;}

		.content .wrapCont{
			width:1024px;
			margin:0 auto;
			display:flex;}
		
			.content .wrapCont .contLeft{
				width:512px;
				color:#fff; }
			
				.content .wrapCont .contLeft figure{
					display:flex;}

				.content .wrapCont .contLeft figcaption{
					display:inline-block;
					vertical-align:top;
					padding-left:20px;
					padding-right:26px; }

				.content .wrapCont .contLeft figcaption strong{
					width:100%;
					border-bottom:1px solid #fff;
					display:inline-block;
					padding-bottom:10px;
					font-size:1.1em;}

				.content .wrapCont .contLeft figcaption p{
					font-size:0.75em;
					padding-top:10px;}

			.content .wrapCont .contLeft > input[type=button]{
				padding:10px 20px;
				background:#0d5c61;
				color:#fff;
				border:none;
				float:right;
				margin-right:145px; }

		/*content Right*/
		
		.contRight form{
			padding-left:55px; }

			.contRight form strong{
				display:inline-block;
				font-size:1.1em;
				padding-bottom:10px;}

			.contRight form  .selectBox{
				padding:10px 0;
				border-top:1px solid #8dbec1;
				border-bottom:1px solid #b2b6b7;
				width:400px; }

				.contRight form  .selectBox dl{
					display:flex;
					padding:3px 0;
					font-family:'돋움';
					font-size:0.75em;}

					.contRight form  .selectBox dl dt{
						padding-top:5px; }

						.contRight form  .selectBox dl input[type=text],
						.contRight form  .selectBox dl select{
							height:30px;
							border:1px solid #ccc;
							margin-left:10px;
							color:#666;
							padding-left:5px; }

						.contRight form  .selectBox dl #carColor,
						.contRight form  .selectBox dl #fule{
							margin-right:20px;
							width:90px;}

						.contRight form  .selectBox dl #start,
						.contRight form  .selectBox dl #end{
							width:150px; }

						.contRight form  .selectBox .start select,
						.contRight form  .selectBox .end select{
							width:75px; }

			.contRight form .okBox{
				text-align:center;
				margin-top:7px; }

				.contRight form .okBox input[type=submit]{
					padding:10px 20px;
					background:#0d5c61;
					color:#fff;
					font-weight:bold;
					border:none; }


		/*guide car*/
		.guide{
			width:1024px;
			margin:0 auto;
			height:500px;
			font-family:'malgun gothic';}
			
			.guide > strong{
				display:block;
				padding-top:30px;
				font-size:1.2em;}
			
			.guide > div{
				display:flex;}

				.guide div figure{
					margin:10px 5px;
					position:relative;}

					.guide div figure .bestImg{
						position:absolute;
						right:10px;
						top:-5px; }

					.guide div figure img:not(.bestImg){
						width:334px; }

					.guide div figure figcaption{
						background:#000;
						color:#fff;
						font-weight:bold;
						text-align:center;
						opacity:0.8;
						padding:5px 0;
						position:absolute;
						width:100%;
						bottom:0;}